      <files>
        <add filePath="~/Scripts/plugins/json2.js"></add>
        <add filePath="~/Scripts/plugins/jquery.blockUI.js"></add>
        <add filePath="~/Scripts/plugins/jquery.numeric.js"></add>
        <add filePath="~/Scripts/plugins/autoNumeric-1.7.4.js"></add>

        <add filePath="~/Scripts/chosen.jquery.js"></add>
        <add filePath="~/Scripts/plugins/twitter-bootstrap-wizard/jquery.bootstrap.wizard.js"></add>
        <add filePath="~/Scripts/plugins/bootstrap-transfer/js/bootstrap-transfer.js"></add>
        <add filePath="~/Scripts/password_unmask.js"></add>
        <add filePath="~/Scripts/plugins/jquery.multiselect/jquery.multiselect.js"></add>
        <add filePath="~/Scripts/plugins/jquery.multiselect/jquery.multiselect.filter.js"></add>
        <add filePath="~/Scripts/plugins/jquery-cookie/jquery.cookie.js"></add>
      </files>
    </add>

    <add bundlePath="~/js/jqgrid">
      <files>
        <add filePath="~/Scripts/jquery.jqGrid.src.js"></add>
        <add filePath="~/Scripts/jqGrid/Grid.js"></add>
      </files>
    </add>

    <add bundlePath="~/js/localizations-MK">
      <files>
        <add filePath="~/Scripts/localization/jqGrid/grid.locale-mk.js"></add>
        <add filePath="~/Scripts/localization/jqueryui/jquery.ui.datepicker-mk.js"></add>
        <add filePath="~/Scripts/localization/ko.validation/mk-MK.js"></add>
      </files>
    </add>

    <add bundlePath="~/js/localizations-SQ">
      <files>
        <add filePath="~/Scripts/localization/jqGrid/grid.locale-sq.js"></add>
        <add filePath="~/Scripts/localization/jqueryui/jquery.ui.datepicker-sq.js"></add>
        <add filePath="~/Scripts/localization/ko.validation/sq-SQ.js"></add>
      </files>
    </add>

    <add bundlePath="~/js/app">
      <files>
        <add filePath="~/Scripts/framework/ns.config.js"></add>
        <add filePath="~/Scripts/framework/ns.utils.js"></add>
        <add filePath="~/Scripts/framework/ns.controls.ajaxlogin.js"></add>
        <add filePath="~/Scripts/framework/ns.controls.js"></add>
        <add filePath="~/Scripts/framework/ns.validators.js"></add>
        <add filePath="~/Scripts/framework/ko.extensions.js"></add>
        <add filePath="~/Scripts/framework/notification.js"></add>
      </files>
      <directories>
        <add directoryPath="~/Scripts/app/repository/" searchPattern="*.js"></add>
        <add directoryPath="~/Scripts/app/viewmodels/" searchPattern="*.js"></add>
        <add directoryPath="~/Scripts/app/widgets/" searchPattern="*.js"></add>
      </directories>
    </add>

