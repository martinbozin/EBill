<?xml version="1.0"?>
<bundleConfig ignoreIfDebug="false" ignoreIfLocal="false">

  <cssBundles>
    <add bundlePath="~/css/bootstrap">
      <files>
        <add filePath="~/content/bootstrap.css"></add>
        <add filePath="~/content/bootstrap-responsive.css"></add>
      </files>
    </add>

    <add bundlePath="~/css/jqueryui">
      <files>
        <add filePath="~/content/jquery.jqGrid/ui.jqgrid.css"></add>
        <add filePath="~/Scripts/jquery-ui-bootstrap/css/jquery-ui-1.9.2.custom.css"></add>
      </files>
    </add>

    <add bundlePath="~/css/controls">
      <files>
        <add filePath="~/content/chosen.css"></add>
        <add filePath="~/Scripts/plugins/bootstrap-transfer/css/bootstrap-transfer.css"></add>
        <add filePath="~/Scripts/plugins/jquery.multiselect/jquery.multiselect.css"></add>
        <add filePath="~/Scripts/plugins/jquery.multiselect/jquery.multiselect.filter.css"></add>
        <add filePath="~/Scripts/plugins/toolTip/tipTip.css"></add>
      </files>
    </add>

    <add bundlePath="~/css/shared">
      <files>
        <add filePath="~/content/site.css"></add>
      </files>
    </add>

    <add bundlePath="~/css/ie">
      <files>
        <add filePath="~/content/ie.css"></add>
        <add filePath="~/Scripts/jquery-ui-bootstrap/css/jquery.ui.1.9.2.ie.css"></add>
      </files>
    </add>
  </cssBundles>

  <jsBundles>

