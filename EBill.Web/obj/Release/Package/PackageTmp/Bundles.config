    </add>

    <add bundlePath="~/js/jqueryui">
      <files>
        <add filePath="~/Scripts/jquery-ui-1.9.2.js"></add>
      </files>
    </add>

    <add bundlePath="~/js/knockout">
      <files>
        <add filePath="~/Scripts/knockout-2.2.1.js"></add>
