@using eDozvoli.Infrastructure.MVC.Notification
@{
    Layout = "_MainLayout.cshtml";
}
<div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
        <div class="container-fluid">
            <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </a>
            <a class="brand" href="#">@Nextsense.ResourcesCommon.ResourceManager.GetResourceValue("_Layout.cshtml.eDozvoli")</a>
            <div class="nav-collapse">
                @Html.Partial("_MainNavigation")
                @Html.Partial("_LoginPartial")
            </div>
            <!--/.nav-collapse -->
        </div>
    </div>
</div>
<div class="container-fluid">
    <div class="row-fluid">
        <div class="span2">
            <div class="well sidebar-nav">
                @Html.Partial("_SidebarNavigation")
            </div>
            <!--/.well -->
        </div>
        <div id="messagewrapper">
            @Html.RenderMessages()
        </div>
        <!--/span-->
        <div class="span10">
            @RenderBody()
        </div>
        <!--/span-->
    </div>
    <!--/row-->
    <hr />
    <footer>
        <p>© Nextsense 2013</p>
    </footer>
</div>

